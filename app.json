{
  "expo": {
    "name": "Child Story",
    "slug": "child_Story",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "scheme": "child_Story",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "package": "com.jsm.childStory-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hafidnid.childStoryapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon3.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1c1549"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "91809e1d-ff62-44ce-988c-a9a40c0d693f"
      }
    }
  }
}
